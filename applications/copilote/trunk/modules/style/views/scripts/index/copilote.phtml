/* Style spécifique pour la navabr INED */

.navbar-default {
    margin-top: 10px;
    background: linear-gradient(to bottom, #1b1c1e 0%, #2c3846 100%) repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: none;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0
}

.navbar-default ul.navbar-nav > li {
    border-collapse: collapse;
    border-left: 1px solid #394251;
    border-right: 1px solid #010000;
    display: table-cell;
    min-height: 55px;
    padding-bottom: 0;
    -moz-transition: -moz-background 0.5s ease 0s;
    -o-transition: -o-background 0.5s ease 0s;
    -webkit-transition: -webkit-background 0.5s ease 0s;
    transition: background 0.5s ease 0s;
    vertical-align: top
}

.navbar-default ul.navbar-nav > li > a {
    border-bottom: 5px solid #fff;
    color: #fff;
    display: table-cell;
    vertical-align: middle;
    font-family: open_sansbold;
    font-size: 1em;
    height: 51px;
    line-height: 1em;
    padding: 10px;
    text-align: center;
    text-transform: uppercase;
    -moz-transition: border-bottom-width 0.2s ease 0s, -moz-background 0.2s ease 0s;
    -o-transition: border-bottom-width 0.2s ease 0s, -o-background 0.2s ease 0s;
    -webkit-transition: border-bottom-width 0.2s ease 0s, -webkit-background 0.2s ease 0s;
    transition: border-bottom-width 0.2s ease 0s, background 0.2s ease 0s;
    width: 100%
}

.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus {
    color: #FFF
}

.navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
