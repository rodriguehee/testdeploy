/* Style spécifique pour mobile */
@media (max-width: 767px) {
    .pull-bottom {
        margin-top: 65px;
    }

    .navbar-collapse {
        max-height: none;
    }

    .navbar-nav {
        width: 100%;
        display: inline;
    }

    .navbar-nav > li:not(.no-list) {
        display: block !important;
    }

    .navbar-nav > li > a {
        width: 100%;
        display: block !important;
    }
}

